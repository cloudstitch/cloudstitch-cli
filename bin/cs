#!/usr/bin/env node
'use strict';

process.title = "cloudstitch";

var prompt = require("prompt");
prompt.message = process.title;
prompt.delimiter = " > "

require('ts-node').register({
    "compilerOptions": {
        // types option has been previously configured
         "types": [
            // add node as an option
            "node"
         ]
    }
});
require('../src/index');
